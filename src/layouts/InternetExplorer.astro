<style scoped is:inline>
  html {
    font-family: system-ui, sans-serif;
    background-color: #f6f6f6;
  }
  code {
    font-family: Menlo, Monaco, Lucida Console, Liberation Mono,
      DejaVu Sans Mono, Bitstream Vera Sans Mono, Courier New, monospace;
  }

  .outdated-notice {
    max-width: 1200px;
    margin: 0;
    box-sizing: border-box;
    width: 100%;
    position: absolute;
    top: 0px;
    bottom: 0px;
    right: 0px;
    left: 0px;
    text-align: center;
    display: -ms-flexbox;
    display: flex;
    justify-content: center;
    align-items: center;
    -ms-flex-align: center;
    flex-direction: column;
    -ms-flex-direction: column;
  }

  .outdated-notice a {
    text-decoration: none;
    text-transform: none;
    font-size: 1em;
    font-weight: 700;
    height: 3em;
    line-height: 3em;
    padding-left: 1em;
    padding-right: 1em;
    border-radius: 6px;
    box-shadow: 0px 4px 8px 0px rgba(0, 0, 0, 0.34);
    background-color: white;
    transition: all 0.3s;
  }

  .outdated-notice a:hover {
    background-color: #057fd7;
    color: white;
  }

  .outdated-notice a svg {
    display: inline-block;
    margin-right: 0.5em;
    vertical-align: middle;
  }
</style>
